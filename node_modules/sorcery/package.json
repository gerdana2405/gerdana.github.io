  "license": "MIT",
  "dependencies": {
    "buffer-crc32": "^0.2.5",
    "minimist": "^1.2.0",
    "sander": "^0.5.0",
    "sourcemap-codec": "^1.3.0"
  },
  "devDependencies": {
    "buble": "^0.5.6",
    "codecov.io": "^0.1.6",
    "coffee-script": "^1.10.0",
    "eslint": "^2.8.0",
    "istanbul": "^0.4.3",
